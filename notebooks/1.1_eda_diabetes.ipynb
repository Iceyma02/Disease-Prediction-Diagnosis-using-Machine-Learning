  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Target distribution\n",
    "print(\"🎯 Target Distribution:\")\n",
    "print(\"=\" * 50)\n",
    "target_counts = diabetes_df['diabetes'].value_counts()\n",
    "target_percentages = diabetes_df['diabetes'].value_counts(normalize=True) * 100\n",
    "\n",
    "fig, (ax1, ax2) = plt.subplots(1, 2, figsize=(15, 6))\n",
    "\n",
    "# Count plot\n",
    "sns.countplot(data=diabetes_df, x='diabetes', ax=ax1, palette=['lightgreen', 'red'])\n",
    "ax1.set_title('Diabetes Distribution', fontsize=14, fontweight='bold')\n",
    "ax1.set_xlabel('Diabetes (0=No, 1=Yes)', fontsize=12)\n",
    "ax1.set_ylabel('Count', fontsize=12)\n",
    "\n",
    "# Add value labels\n",
    "for i, v in enumerate(target_counts):\n",
    "    ax1.text(i, v + 1000, f'{v:,}', ha='center', va='bottom', fontweight='bold', fontsize=12)\n",
    "\n",
    "# Pie chart\n",
    "colors = ['lightgreen', 'red']\n",
    "ax2.pie(target_counts, labels=['No Diabetes', 'Diabetes'], \n",
    "        autopct='%1.1f%%', colors=colors, startangle=90)\n",
    "ax2.set_title('Diabetes Proportion', fontsize=14, fontweight='bold')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "print(f\"\\n📊 Distribution Details:\")\n",
    "print(f\"   • No Diabetes: {target_counts[0]:,} patients ({target_percentages[0]:.1f}%)\")\n",
    "print(f\"   • Diabetes: {target_counts[1]:,} patients ({target_percentages[1]:.1f}%)\")\n",
    "print(f\"   • Dataset has class imbalance - will need handling in modeling\")"
   ]
