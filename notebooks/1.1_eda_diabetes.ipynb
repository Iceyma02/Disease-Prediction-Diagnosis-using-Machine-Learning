   "outputs": [],
   "source": [
    "# Categorical features analysis\n",
    "categorical_features = ['gender', 'hypertension', 'heart_disease', 'smoking_history']\n",
    "\n",
    "print(\"📊 Categorical Features Analysis:\")\n",
    "print(\"=\" * 50)\n",
    "\n",
    "fig, axes = plt.subplots(2, 2, figsize=(20, 12))\n",
    "axes = axes.ravel()\n",
    "\n",
    "for i, feature in enumerate(categorical_features):\n",
    "    # Value counts\n",
    "    value_counts = diabetes_df[feature].value_counts()\n",
    "    \n",
    "    # Create subplot\n",
    "    if feature in ['hypertension', 'heart_disease']:\n",
    "        # Binary features\n",
    "        colors = ['lightblue', 'lightcoral']\n",
    "        labels = ['No', 'Yes']\n",
    "        bars = axes[i].bar([0, 1], value_counts.values, color=colors)\n",
    "        axes[i].set_xticks([0, 1])\n",
    "        axes[i].set_xticklabels(labels)\n",
    "    else:\n",
    "        # Multi-category features\n",
    "        bars = axes[i].bar(range(len(value_counts)), value_counts.values, \n",
    "                          color=plt.cm.Set3(np.arange(len(value_counts))))\n",
    "        axes[i].set_xticks(range(len(value_counts)))\n",
    "        axes[i].set_xticklabels(value_counts.index, rotation=45)\n",
    "    \n",
    "    axes[i].set_title(f'{feature.replace(\"_\", \" \").title()} Distribution', fontweight='bold')\n",
    "    axes[i].set_ylabel('Count')\n",
    "    \n",
    "    # Add value labels\n",
    "    for bar, value in zip(bars, value_counts.values):\n",
    "        axes[i].text(bar.get_x() + bar.get_width()/2, bar.get_height() + 100, \n",
    "                    f'{value:,}', ha='center', va='bottom', fontweight='bold')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# Print categorical value distributions\n",
    "print(\"\\n📋 Categorical Value Distributions:\")\n",
    "for feature in categorical_features:\n",
    "    print(f\"\\n{feature.replace('_', ' ').title()}:\")\n",
    "    value_counts = diabetes_df[feature].value_counts()\n",
    "    for value, count in value_counts.items():\n",
    "        percentage = (count / len(diabetes_df)) * 100\n",
    "        print(f\"   • {value}: {count:,} ({percentage:.1f}%)\")"
   ]
